    function displayAnswered(ques, ans, position) {
        
        document.getElementById('actQu' + ques).style.display = "none";
        document.getElementById('answer' + ques).innerText = ans;
        
        var prev = Number(ques) - 1;
        var prevPrev = Number(prev) - 1;
        var prevPrevPrev = Number(prevPrev) - 1;
        var prevPrevPrevPrev = Number(prevPrevPrev) - 1;
        var prevPrevPrevPrevPrev = Number(prevPrevPrevPrev) - 1;
        
        var next = Number(ques) + 1;
          
        if(position == "first") {
            document.getElementById('ansQu' + ques).style.display = "block";
            if(document.getElementById('answer' + next).innerText == "{{Answer}}") {
                document.getElementById('actQu' + next).style.display = "block";                  
            }
        }

        if(position == "middle") {
            if(document.getElementById('answer' + ques).innerText != "{{Answer}}") {
                if(prev > 0) {
                    if(document.getElementById('answer' + prev).innerText != "{{Answer}}") {
                        if(document.getElementById('answer' + prev).innerText != "{{Hide}}") {
                            document.getElementById('ansQu' + ques).style.display = "block";
                            displayNextAnswer(ques);
                            displayNextQuestion(ques);
                        }
                    }
                }
                if(prevPrev > 0) {
                    if(document.getElementById('answer' + prevPrev).innerText != "{{Answer}}") {
                        if(document.getElementById('answer' + prevPrev).innerText != "{{Hide}}") {
                            document.getElementById('ansQu' + ques).style.display = "block";
                            displayNextAnswer(ques);
                            displayNextQuestion(ques);
                        }
                    }
                }
                if(prevPrevPrev > 0) {
                    if(document.getElementById('answer' + prevPrevPrev).innerText != "{{Answer}}") {
                        if(document.getElementById('answer' + prevPrevPrev).innerText != "{{Hide}}") {
                            document.getElementById('ansQu' + ques).style.display = "block";
                            displayNextAnswer(ques);
                            displayNextQuestion(ques);
                        }
                    }
                }
                if(prevPrevPrevPrev > 0) {
                    if(document.getElementById('answer' + prevPrevPrevPrev).innerText != "{{Answer}}") {
                        if(document.getElementById('answer' + prevPrevPrevPrev).innerText != "{{Hide}}") {
                            document.getElementById('ansQu' + ques).style.display = "block";
                            displayNextAnswer(ques);
                            displayNextQuestion(ques);
                        }
                    }
                }
                if(prevPrevPrevPrevPrev > 0) {
                    if(document.getElementById('answer' + prevPrevPrevPrevPrev).innerText != "{{Answer}}") {
                        if(document.getElementById('answer' + prevPrevPrevPrevPrev).innerText != "{{Hide}}") {
                            document.getElementById('ansQu' + ques).style.display = "block";
                            displayNextAnswer(ques);
                            displayNextQuestion(ques); 
                        }
                    }
                } 
            }     
        }
        
        if(position == "last") {
            if(document.getElementById('answer' + prev).innerText != "{{Answer}}" && document.getElementById('answer' + ques).innerText != "{{Answer}}") {
                document.getElementById('ansQu' + ques).style.display = "block";    
            } 
        }
    }
    
    function displayNextQuestion(ques) {
        
        var next = Number(ques) + 1;
        var nextNext = Number(next) + 1;
        var nextNextNext = Number(nextNext) + 1;
        var nextNextNextNext = Number(nextNextNext) + 1;
        var nextNextNextNextNext = Number(nextNextNextNext) + 1;
        var nextNextNextNextNextNext = Number(nextNextNextNextNext) + 1;
        
        if(document.getElementById('answer' + next).innerText == "{{Answer}}") {
            document.getElementById('actQu' + next).style.display = "block";
        } else {
            if(document.getElementById('answer' + nextNext).innerText == "{{Answer}}") {
                document.getElementById('actQu' + nextNext).style.display = "block";
            } else {
                if(document.getElementById('answer' + nextNextNext).innerText == "{{Answer}}") {
                    document.getElementById('actQu' + nextNextNext).style.display = "block";    
                } else {
                    if(document.getElementById('answer' + nextNextNextNext).innerText == "{{Answer}}") {
                        document.getElementById('actQu' + nextNextNextNext).style.display = "block";    
                    } else {
                        if(document.getElementById('answer' + nextNextNextNextNext).innerText == "{{Answer}}") {
                            document.getElementById('actQu' + nextNextNextNextNext).style.display = "block";    
                        } else {
                            if(document.getElementById('answer' + nextNextNextNextNextNext).innerText == "{{Answer}}") {
                                document.getElementById('actQu' + nextNextNextNextNextNext).style.display = "block";    
                            }
                        }
                    }
                }
            }
        }
    }
   
    
    function displayNextAnswer(ques) {
        var next = Number(ques) + 1;
        var nextNext = Number(next) + 1;
        var nextNextNext = Number(nextNext) + 1;
        var nextNextNextNext = Number(nextNextNext) + 1;
        var nextNextNextNextNext = Number(nextNextNextNext) + 1;
        
        if(next < 30) {
            if(document.getElementById('answer' + next).innerText != "{{Answer}}" && document.getElementById('answer' + next).innerText != "{{Hide}}") {
                document.getElementById('ansQu' + next).style.display = "block";
            }
        }
        if(nextNext < 30) {
            if(document.getElementById('answer' + nextNext).innerText != "{{Answer}}" && document.getElementById('answer' + nextNext).innerText != "{{Hide}}") {
                document.getElementById('ansQu' + nextNext).style.display = "block";
            }
        }
        if(nextNextNext < 30) {
            if(document.getElementById('answer' + nextNextNext).innerText != "{{Answer}}" && document.getElementById('answer' + nextNextNext).innerText != "{{Hide}}") {
                document.getElementById('ansQu' + nextNextNext).style.display = "block";
            }
        }
        if(nextNextNextNext < 30) {
            if(document.getElementById('answer' + nextNextNextNext).innerText != "{{Answer}}" && document.getElementById('answer' + nextNextNextNext).innerText != "{{Hide}}") {
                document.getElementById('ansQu' + nextNextNextNext).style.display = "block";
            }
        }
        if(nextNextNextNextNext < 30) {
            if(document.getElementById('answer' + nextNextNextNextNext).innerText != "{{Answer}}" && document.getElementById('answer' + nextNextNextNextNext).innerText != "{{Hide}}") {
