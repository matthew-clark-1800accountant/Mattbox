                // function to hide divs containing unchecked checkboxes
                function retractProducts() {
                    $('#ProdRetract').hide(); //hide retract button
                    $('#ProdCust').show(); //show expand button

                    // get all the divs with class 'form-group' and 'prodgroup'
                    var divs = document.querySelectorAll('div.form-group.prodgroup');
                    
                    divs.forEach(function(div) {
                        // get the checkbox within the div
                        var checkbox = div.querySelector('input[type=checkbox]');
                        var dataId = checkbox.getAttribute('data-id');
                        dataId = dataId.substring(0, dataId.length-5);

                        // check whether the checkbox is unchecked
                        if (checkbox && (!checkbox.checked || dataIdsToHide.includes(dataId))) {
                            // hide the parent div
                            div.style.display = 'none';
                        } else if(checkbox && checkbox.checked && !dataIdsToHide.includes(dataId)){
                            div.style.display = 'block';
                        }
                    });
                    updateProductTotal(); //would like to get rid of this dependency eventually
                }

                function expandProducts() {
                    $('#ProdCust').hide(); //hide expand button
                    $('#ProdRetract').show(); //show retract button
                    //$('.prodgroup').show(); //display every product
                    var divs = document.querySelectorAll('div.form-group.prodgroup');
                    
                    divs.forEach(function(div) {
                        // get the checkbox within the div
                        var checkbox = div.querySelector('input[type=checkbox]');
                        var dataId = checkbox.getAttribute('data-id');
                        dataId = dataId.substring(0, dataId.length-5);

                        // check whether the checkbox is unchecked
                        if (checkbox && !dataIdsToHide.includes(dataId)) {
                            // hide the parent div
                            div.style.display = 'block';
                        } else if(checkbox){
                            div.style.display = 'none';
                        }
                    });
                    updateProductTotal(); //would like to get rid of this dependency eventually

                }
                
                function updateTotal(doAmount,element) {
                    
                    // get existing total amount
                    var theValue = parseFloat(document.getElementById('holdTotal').value);
                    
                    doAmount = parseFloat(doAmount);
                    
                    if (element.checked) {
                        // if checked add to total
                        theValue = theValue + doAmount;
                    } else {
                        // if unchecked subtract from total
                        theValue = theValue - doAmount;
                    }
                    
                    // set new amount
                    document.getElementById('holdTotal').value = parseFloat(theValue).toFixed(2);
                    
                }
                
