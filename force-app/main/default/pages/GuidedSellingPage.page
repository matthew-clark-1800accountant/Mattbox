    var formationDate;
    function getDaysSinceFormation(){
        if(formationDate){
            var startDate = parseDate(formationDate).valueOf();
            var today = new Date().valueOf();
            var msDifference = today - startDate;
            return Math.floor(msDifference / (1000*60*60*24));
        } else {
            return null;
        }
    }
