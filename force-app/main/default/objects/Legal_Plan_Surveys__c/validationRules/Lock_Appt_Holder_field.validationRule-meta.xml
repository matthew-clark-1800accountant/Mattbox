<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Lock_Appt_Holder_field</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISCHANGED( Appt_Holder__c),
NOT(ISBLANK(PRIORVALUE(Appt_Holder__c))))</errorConditionFormula>
    <errorMessage>Can&apos;t change the Appt Holder</errorMessage>
</ValidationRule>
