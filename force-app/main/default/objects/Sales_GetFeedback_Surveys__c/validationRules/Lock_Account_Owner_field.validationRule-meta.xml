<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Lock_Account_Owner_field</fullName>
    <active>false</active>
    <errorConditionFormula>AND( 
ISCHANGED(  Account_Owner__c  ), 
NOT(ISBLANK(PRIORVALUE(  Account_Owner__c  ))))</errorConditionFormula>
    <errorMessage>Can&apos;t change Account Owner</errorMessage>
</ValidationRule>
